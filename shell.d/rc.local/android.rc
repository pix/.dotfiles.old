ADT_BUNDLE_ROOT="$HOME/Devel/Android/sdk/adt-bundle"
export ANDROID_HOME="$ADT_BUNDLE_ROOT/sdk"
for i in "eclipse" "sdk/platform-tools/"; do
    [ -d "${ADT_BUNDLE_ROOT}/${i}" ] && addpath "${ADT_BUNDLE_ROOT}/${i}"
done


if [ -d "$HOME/Devel/Android/sdk/android-ndk" ]; then
    export ANDROID_NDK_ROOT="$HOME/Devel/Android/sdk/android-ndk"
fi
